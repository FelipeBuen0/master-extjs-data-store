{
  "name": "JsDaysDataStore",
  "namespace": "JsDaysDataStore",
  "version": "1.0.0",
  "branch": "",
  "framework": "ext",
  "template": "moderndesktopminimal",
  "indexHtmlPath": "index.html",
  "requires": [
    "font-awesome"
  ],
  "builds": {
    "desktop": {
      "toolkit": "modern",
      "theme": "theme-material",
      "sass": {
        "generated": {
          "var": "${build.id}/sass/save.scss",
          "src": "${build.id}/sass/save"
        }
      }
    }
  },
  "classpath": ["app/${build.id}/src"],
  "overrides": ["app/${build.id}/overrides"],
  "sass": {
    "namespace": "JsDaysDataStore",
    "etc": ["sass/etc/all.scss"],
    "var": ["app/${build.id}/sass/var.scss"],
    "src": ["app/${build.id}/sass/src.scss"]
  },
  "resources": [
    {
      "path": "resources/${build.id}"
    }
  ],
  "output": {
    "base": "${workspace.build.dir}/${build.environment}/${app.name}",
    "page": "index.html",
    "manifest": "generatedFiles/${build.id}.json",
    "js": {
      "path": "generatedFiles/${build.id}/app.js",
      "version": "NEXT",
      "input": "NEXT",
      "output": "NEXT"
    },
    "appCache": {
      "enable": false
    },
    "resources": {
      "path": "${build.id}/resources",
      "shared": "resources"
    }
  },
  "fashion": {
    "missingParameters": "error",
    "inliner": {
      "enable": false
    }
  },
  "classic": {
    "js": []
  },
  "modern": {
    "js": []
  },
  "css": [
    {
      "path": "${build.out.css.path}",
      "bundle": true,
      "exclude": ["fashion"]
    }
  ],
  "loader": {
    "cache": false,
    "cacheParam": "_dc"
  },
  "production": {
    "js": [
      {
        "path": "app.js",
        "bundle": true
      },
      {
        "path": "${app.webpack.bundle}",
        "compress": false
      }
    ],
    "api": {
      "url": "https://newsapi.org/v2",
      "apiToken": "b75acf5597864014a62c613923efe32d"
    },
    "output": {
      "appCache": {
        "enable": false,
        "path": "cache.appcache"
      }
    },
    "material": {
      "toolbar": {
        "color": "#578548",
        "dynamic": true
      }
    },
    "security": {
      "token": {
        "key": "inobag_auth"
      }
    },
    "watch": {
      "delay": 250
    },
    "uploadConfig": {
      "allowedExtentions": ".jpg",
      "maxSize": 5242880,
      "image": {
        "maxWidth": 1920,
        "maxHeight": 1080
      }
    },
    "message": {
      "image": {
        "maxWidth": 1280,
        "maxHeight": 720
      }
    },
    "loader": {
      "cache": "${build.timestamp}"
    },
    "cache": {
      "enable": true
    },
    "compressor": {
      "type": "yui"
    }
  },
  "testing": {
    "js": [
      {
        "path": "app.js",
        "bundle": true
      },
      {
        "path": "${app.webpack.bundle}",
        "compress": false
      }
    ],
    "material": {
      "toolbar": {
        "color": "#578548",
        "dynamic": true
      }
    }
  },
  "development": {
    "js": [
      {
        "path": "app.js",
        "bundle": true
      },
      {
        "path": "main.js"
      }
    ],
    "api": {
      "url": "https://newsapi.org/v2",
      "apiToken": "b75acf5597864014a62c613923efe32d"
    },
    "material": {
      "toolbar": {
        "color": "#578548",
        "dynamic": true
      }
    },
    "watch": {
      "delay": 250
    },
    "uploadConfig": {
      "allowedExtentions": ".jpg",
      "maxSize": 5242880,
      "image": {
        "maxWidth": 1920,
        "maxHeight": 1080
      }
    },
    "message": {
      "image": {
        "maxWidth": 1280,
        "maxHeight": 720
      }
    }
  },
  "bootstrap": {
    "base": "${app.dir}",
    "manifest": "generatedFiles/${build.id}.json",
    "microloader": "generatedFiles/bootstrap.js",
    "css": "generatedFiles/bootstrap.css"
  },
  "cache": {
    "enable": false,
    "deltas": "${build.id}/deltas"
  },
  "appCache": {
    "cache": ["index.html"],
    "network": ["*"],
    "fallback": []
  },
  "archivePath": "archive/${build.id}",
  "ignore": ["(^|/)CVS(/?$|/.*?$)"],
  "id": "bfee2f41-8e7f-4142-8470-43b3f5364f8a"
}
